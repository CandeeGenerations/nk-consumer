{
  "name": "nk-consumer",
  "version": "1.2.3",
  "description": "NK Tools Consumer",
  "homepage": "https://github.com/CandeeGenerations/nk-consumer#readme",
  "bugs": {
    "url": "https://github.com/CandeeGenerations/nk-consumer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:CandeeGenerations/nk-consumer.git"
  },
  "license": "MIT",
  "author": "Candee Generations <admin@candeegenerations.com>",
  "type": "commonjs",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production tsc && tsc-alias",
    "eslint": "eslint 'src/**/*.ts'",
    "fix": "yarn run eslint && yarn run prettier --cache",
    "preinstall": "sh scripts/check-versions.sh && npx only-allow yarn",
    "install:all": "fnm use; yarn install;",
    "prepare": "husky install",
    "prettier": "prettier 'src/**/*.ts' --cache --check --write --ignore-unknown .",
    "prettier:ci": "prettier 'src/**/*.ts' --check .",
    "release": "standard-version -a -- --release-as minor",
    "start": "doppler run --command='NODE_ENV=development ts-node-dev --respawn -r tsconfig-paths/register src/index.ts'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.650.0",
    "@aws-sdk/client-ses": "^3.650.0",
    "dayjs": "^1.11.13",
    "express": "^4.20.0",
    "handlebars": "^4.7.8",
    "lodash": "^4.17.21",
    "mimetext": "^3.0.24",
    "puppeteer": "^23.3.0",
    "sqs-consumer": "^11.0.2",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.10.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.4",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "sort-package-json": "^2.10.1",
    "standard-version": "^9.5.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.5.0"
  },
  "packageManager": "yarn@4.4.1",
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=4.0.0"
  }
}
