{
  "name": "nk-consumer",
  "version": "1.1.0",
  "description": "NK Tools Consumer",
  "repository": "git@github.com:CandeeGenerations/nk-consumer.git",
  "author": "Tyler Candee <tyler@candeegenerations.com>",
  "license": "MIT",
  "main": "index.js",
  "type": "module",
  "packageManager": "yarn@4.4.0",
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production tsc -p tsconfig.build.json",
    "start": "doppler run --command='NODE_ENV=development tsc-watch -p tsconfig.build.json --onSuccess \"node dist/index.js\"'",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "release": "standard-version -a -- --release-as minor",
    "prepare": "husky",
    "postinstall": "husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.637.0",
    "@aws-sdk/client-ses": "^3.637.0",
    "axios": "^1.7.5",
    "dayjs": "^1.11.13",
    "express": "^4.19.2",
    "handlebars": "^4.7.8",
    "lodash": "^4.17.21",
    "mimetext": "^3.0.24",
    "node-cron": "^3.0.3",
    "puppeteer": "^23.1.1",
    "sqs-consumer": "^11.0.2",
    "ts-pattern": "^5.3.1",
    "uuid": "^10.0.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.9.1",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.0",
    "@types/node-cron": "^3.0.11",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "tsc-files": "^1.1.4",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0"
  }
}
